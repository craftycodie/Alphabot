{
  "name": "alphabot",
  "version": "1.6.4",
  "description": "Modular Discord Bot",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "tsc && node dist/src/index.js",
    "test": "jest --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/craftycodie/Alphabot.git"
  },
  "author": "craftycodie",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/craftycodie/Alphabot/issues"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "globals": {
      "ts-jest": {
