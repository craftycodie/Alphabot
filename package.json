    "spotify-web-api-node": "^5.0.2",
    "twit": "^2.2.11"
  },
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@types/mockingoose": "^2.13.0",
    "jest": "^27.0.6",
    "jest-ts-auto-mock": "^2.0.0",
    "ts-auto-mock": "^3.3.5",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "tslint": "^6.1.3",
    "ttypescript": "^1.5.12",
    "typescript": "^4.3.5"
  }
}
